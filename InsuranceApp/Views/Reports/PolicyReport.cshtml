
@{
    ViewBag.Title = "PolicyReport";
    Layout = "~/Views/Shared/AdminLayout.cshtml";
}



<iframe id="iframeReportViewer" frameborder="0" width="1100" height="1000" style="overflow:hidden" scrolling="no"></iframe>
<script>
        @if (Session["ReportData"] !=null)
        {
           <text>ShowReport();</text>;
        }
        function ShowReport() {
            var url = "../Reports/Viewer/PolicyReportViewer.aspx";
            var myframe = document.getElementById("iframeReportViewer");
            if (myframe != null) {
                if (myframe.src) {
                    myframe.src = url;
                } else if (myframe.contentWindow != null && myframe.contentWindow.location != null) {
                    myframe.contentWindow.location = url;
                }
                else {
                    myframe.setAttribute('src', url);
                }

            }
        }
</script>





